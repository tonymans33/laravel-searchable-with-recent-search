{
    "name": "tonymans33/laravel-searchable-with-recent-search",
    "description": "Pragmatically search through models and other sources, with a recent search feature. build on top of Spatie Searchable.",
    "keywords": [
        "laravel-searchable-with-recent-search"
    ],
    "homepage": "https://github.com/tonymans33/laravel-searchable-with-recent-search",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Vanderbist",
            "email": "alex@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Tony Mansour",
            "email": "mansourtony44@gmail.com",
            "homepage": "https://github.com/tonymans33",
            "role": "Backend Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.78|^9.0|^10.0|^11.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.27|^7.0|^8.0",
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^9.3|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Tonymans33\\SearchableWithRecent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tonymans33\\SearchableWithRecent\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tonymans33\\SearchableWithRecent\\Providers\\RecentSearchServiceProvider"
            ]
        }
    }
}
